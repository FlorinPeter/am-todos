{
  "name": "am-todos",
  "version": "1.9.0",
  "description": "AI-powered todo application that transforms GitHub repositories into intelligent task management systems",
  "license": "Apache-2.0",
  "author": "Agentic Markdown Todos Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/am-todos.git"
  },
  "keywords": [
    "todo",
    "markdown",
    "ai",
    "github",
    "task-management",
    "react",
    "typescript"
  ],
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-markdown": "^6.3.3",
    "@codemirror/language": "^6.11.2",
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@lezer/highlight": "^1.2.1",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.16.3",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@uiw/codemirror-themes": "^4.24.1",
    "@uiw/react-codemirror": "^4.24.1",
    "@uiw/react-markdown-preview": "^5.1.4",
    "js-yaml": "^4.1.0",
    "node-emoji": "^2.2.0",
    "postcss": "^8.4.49",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.3"
  },
  "scripts": {
    "start": "vite --host 0.0.0.0",
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all-fast": "vitest run --config=vitest.fast.config.mjs",
    "test:basic": "vitest run src/__tests__/FeatureValidation.test.ts src/__tests__/ComponentBasics.test.tsx",
    "test:integration": "INTEGRATION_TEST=true vitest run src/services/__tests__/githubService.test.ts",
    "test:github": "INTEGRATION_TEST=true vitest run src/services/__tests__/githubService.test.ts",
    "test:retry": "INTEGRATION_TEST=true vitest run src/__tests__/retryLogic.test.ts",
    "test:github-basic": "node test-github-integration.js",
    "test:github-stress": "node test-stress-github.js",
    "test:last-file": "node test-last-file-deletion.js",
    "dev": "vite",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "dead-code": "ts-unused-exports tsconfig.json --excludeDeclarationFiles",
    "dead-deps": "depcheck --ignores='@types/*,vite,vitest'",
    "dead-code:all": "npm run dead-code && npm run dead-deps",
    "dead-code:analyze": "node scripts/analyze-dead-code.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.16.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.0.0",
    "ts-unused-exports": "^11.0.1",
    "vite": "^7.0.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
